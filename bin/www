#!/usr/bin/env node
var app = require('../app');

app.set('port', process.env.PORT || 3000);
app.set('debug', 13000)

app.locals.AUTHCODE_URL = process.env.AUTHCODE_URL || 'http://app.1self.dev/auth/authorization';
app.locals.TOKEN_URL = process.env.TOKEN_URL || 'http://app.1self.dev';

var server = app.listen(app.get('port'), function() {
  console.log('Express server listening on port ' + server.address().port);
});
	